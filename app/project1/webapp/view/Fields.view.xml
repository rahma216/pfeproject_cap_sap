<mvc:View
    xmlns:vbm="sap.ui.vbm"
    xmlns="sap.uxap"
    xmlns:m="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="app.project1.controller.Fields"
    xmlns:core="sap.ui.core"
>
    <ObjectPageLayout
        id="ObjectPageLayout"
        showTitleInHeaderContent="true"
        alwaysShowContentHeader="false"
        preserveHeaderStateOnScroll="false"
        headerContentPinnable="true"
        isChildPage="true"
        upperCaseAnchorBar="false"
    >
        <headerTitle>
            <ObjectPageDynamicHeaderTitle>
                <expandedHeading>
                    <m:HBox>
                        <m:Title
                            text="{mainModel>name}"
                            wrapping="true"
                            id="rahma1"
                        />
                        <m:ObjectMarker
                            type="Favorite"
                            class="sapUiTinyMarginBegin"
                        />
                    </m:HBox>
                </expandedHeading>
                <navigationActions>
                    <m:OverflowToolbarButton
                        type="Transparent"
                        icon="sap-icon://decline"
                        press="handleClose"
                        tooltip="Close middle column"
                        visible="true"
                    />
                </navigationActions>

                <actions>
                    <m:ToggleButton
                        text="Edit"
                        type="Emphasized"
                        press=".onEditToggleButtonPress"
                    />
                    <m:Button
                        text="Delete"
                        type="Transparent"
                    />
                    <m:Button
                        text="Copy"
                        type="Transparent"
                    />
                    <m:Button
                        icon="sap-icon://action"
                        type="Transparent"
                    />
                </actions>
            </ObjectPageDynamicHeaderTitle>
        </headerTitle>

        <headerContent>
            <m:HBox
                wrap="Wrap"
                fitContainer="false"
                alignItems="Stretch"
            >
                <f:Avatar
                    displaySize="L"
                    displayShape="Circle"
                    class="sapUiTinyMarginEnd"
                />

                <m:VBox
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                    items=""
                >
                    <form:SimpleForm
                        maxContainerCols="2"
                        editable="false"
                        layout="ResponsiveGridLayout"
                        labelSpanL="12"
                        labelSpanM="12"
                        emptySpanL="0"
                        emptySpanM="0"
                        columnsL="1"
                        columnsM="1"
                    >
                        <form:content>
                            <m:Label text="Entity ID" />
                            <m:Text
                                id="fldd"
                                text="{mainModel>ID}"
                                visible="false"
                            />
                            <m:Label text="Entity Name" />
                            <m:Text id="rahma" text="{mainModel>name}" />
                        </form:content>
                    </form:SimpleForm>
                </m:VBox>
            </m:HBox>
        </headerContent>

        <sections>
            <ObjectPageSection>
                <subSections>
                    <ObjectPageSubSection
                        title="{i18n>section1}"
                    >
                        <blocks>
                            <m:VBox>
                                <form:SimpleForm
                                    maxContainerCols="2"
                                    editable="false"
                                    layout="ResponsiveGridLayout"
                                    labelSpanL="12"
                                    labelSpanM="12"
                                    emptySpanL="0"
                                    emptySpanM="0"
                                    columnsL="1"
                                    columnsM="1"
                                    id="form"
                                >
                                    <form:content>
                                        <m:VBox
                                            id="parentvbox"
                                            justifyContent="Center"
                                        >
                                            <m:HBox id="fields">
                                                <m:ComboBox
                                                    id="key"
                                                    placeholder="PrimaryKey"
                                                    class="sapUiSmallMarginBottom"
                                                >
                                                    <core:Item
                                                        key="true"
                                                        text="true"
                                                    />
                                                    <core:Item
                                                        key="false"
                                                        text="false"
                                                    />
                                                </m:ComboBox>

                                                <m:Input
                                                    id="field"
                                                    value=""
                                                    placeholder="EnterYourField"
                                                    width="100%"
                                                />
                                                <m:ComboBox
                                                    placeholder="Type"
                                                    class="sapUiSmallMarginBottom"
                                                    id="idComboBoxSuccess"
                                                    items="{ path: 'actions>/Actions' }"
                                                >
                                                    <core:ListItem
                                                        key="{actions>type}"
                                                        text="{actions>type}"
                                                    />
                                                </m:ComboBox>
                                                <m:MultiComboBox
                                                    placeholder="Annotations"
                                                    class="sapUiSmallMarginBottom"
                                                    id="annotations"
                                                    items="{ path: 'annotations>/Annotations' }"
                                                >
                                                    <core:ListItem
                                                        key="{annotations>name}"
                                                        text="{annotations>name}"
                                                    />
                                                </m:MultiComboBox>

                                                <m:Button
                                                    id="confirmCreate"
                                                    text="Create"
                                                    press=".onCreate2"
                                                    type="Emphasized"
                                                />
                                            </m:HBox>
                                        </m:VBox>
                                        <m:Label text="Entity Name" />
                                    </form:content>
                                </form:SimpleForm>
                            </m:VBox>
                        </blocks>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>
            <ObjectPageSection
                title="{i18n>section2}"
                showTitle="false"
            >
                <subSections>
                    <ObjectPageSubSection>
                        <blocks>
                            <m:VBox>
                                <m:Button
                                    id="invisibleButton"
                                    text=""
                                    press="onInvisibleButtonPress"
                                    visible="false"
                                />

                                <m:Table
                                    items="{ path: 'mainModel>fields' }"
                                    id="table1"
                                    mode="SingleSelectLeft"
                                    selectionChange="onSelect"
                                >
                                    <m:headerToolbar>
                                        <m:OverflowToolbar>
                                            <m:content>
                                                <m:Title
                                                    text="{i18n>section22}"
                                                    level="H2"
                                                />
                                                <m:ToolbarSpacer />
                                                <m:Button
                                                    id="editButton2"
                                                    icon="sap-icon://edit"
                                                    tooltip="Edit"
                                                    visible="true"
                                                    press="onOpenAddDialog2"
                                                >
                                                    <m:layoutData>
                                                        <m:OverflowToolbarLayoutData
                                                            priority="NeverOverflow"
                                                        />
                                                    </m:layoutData>
                                                </m:Button>
                                                <m:Button
                                                    id="delete"
                                                    icon="sap-icon://delete"
                                                    tooltip="Delete"
                                                    visible="true"
                                                    press=".onDelete"
                                                >
                                                    <m:layoutData>
                                                        <m:OverflowToolbarLayoutData
                                                            priority="NeverOverflow"
                                                        />
                                                    </m:layoutData>
                                                </m:Button>
                                            </m:content>
                                            <m:dependents>
                                   <m:Dialog 
                                                    id="OpenDialog2"
                                                    title="Update Your Field"
                                                    stretch="false"
                                                >
                                                    <m:VBox
                                                        class="sapUiContentPadding sapUiSmallMargin"
                                                    >
                                                        <m:Label
                                                            text="Is Key"
                                                        />
                                                        <m:Input
                                                            id="iskey"
                                                            placeholder="Enter key"
                                                            valueLiveUpdate="true"
                                                            required="true"
                                                            class="sapUiSmallMarginBottom"
                                                        />
                                                        <m:Label text="Value" />
                                                        <m:Input
                                                            id="value"
                                                            placeholder="Enter value"
                                                            valueLiveUpdate="true"
                                                            required="true"
                                                            class="sapUiSmallMarginBottom"
                                                        />
                                                        <m:Label text="Type" />
                                                        <m:ComboBox
                                                            id="idComboBoxupdate"
                                                            placeholder="Select Type"
                                                            class="sapUiSmallMarginBottom"
                                                            items="{ path: 'actions>/Actions' }"
                                                            width="100%"
                                                            required="true"
                                                        >
                                                            <core:ListItem
                                                                key="{actions>type}"
                                                                text="{actions>type}"
                                                            />
                                                        </m:ComboBox>
                                                        <m:Label
                                                            text="Annotations"
                                                        />
                                                        <m:MultiComboBox
                                                            id="annotationsupdate"
                                                            placeholder="Select Annotations"
                                                            class="sapUiSmallMarginBottom"
                                                            items="{ path: 'annotations>/Annotations' }"
                                                            width="100%"
                                                        >
                                                            <core:ListItem
                                                                key="{annotations>name}"
                                                                text="{annotations>name}"
                                                            />
                                                        </m:MultiComboBox>
                                                    </m:VBox>
                                                    <m:buttons>
                                                        <m:Button
                                                            id="confirmCreate2"
                                                            text="{i18n>create1}"
                                                            press=".onUpdate"
                                                            type="Accept"
                                                        />
                                                        <m:Button
                                                            id="cancelCreate2"
                                                            text="{i18n>cancel1}"
                                                            press="onCancelDialog"
                                                            type="Reject"
                                                        />
                                                    </m:buttons>
                                                </m:Dialog>
                                            </m:dependents>
                                        </m:OverflowToolbar>
                                    </m:headerToolbar>

                                    <m:items>
                                        <m:ColumnListItem
                                            type="Active"
                                            id="item1"
                                            press=".onSupplierPress"
                                        >
                                            <m:cells>
                                                <m:Text
                                                    id="id4"
                                                    text="{mainModel>value}"
                                                />
                                                <m:Text
                                                    id="id5"
                                                    text="{mainModel>type}"
                                                />
                                                <m:Text
                                                    id="id6"
                                                    text="{mainModel>annotations}"
                                                />
                                                <m:Text
                                                    id="primkey"
                                                    text="{mainModel>iskey}"
                                                />
                                            </m:cells>
                                        </m:ColumnListItem>
                                    </m:items>
                                    <m:columns>
                                        <m:Column>
                                            <m:header>
                                                <m:Label text="value" />
                                            </m:header>
                                        </m:Column>
                                        <m:Column>
                                            <m:header>
                                                <m:Label text="type" />
                                            </m:header>
                                        </m:Column>
                                        <m:Column>
                                            <m:header>
                                                <m:Label text="annotations" />
                                            </m:header>
                                        </m:Column>
                                        <m:Column>
                                            <m:header>
                                                <m:Label text="PrimaryKey" />
                                            </m:header>
                                        </m:Column>
                                    </m:columns>
                                </m:Table>
                            </m:VBox>
                        </blocks>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>
            <ObjectPageSection
             title="{i18n>Section3}"
                showTitle="false"
            >
                <subSections>
                    <ObjectPageSubSection>
                        <m:VBox>
                            <m:Label text="{i18n>Section33}" />
                            <m:Input
                                id="sourceInput"
                                textFormatMode="KeyValue"
                                placeholder="Source Entity"
                                showSuggestion="true"
                                showValueHelp="true"
                                value="{mainModel>name}"
                                editable="false"
                            />
                            <m:Input
                                id="targetInput"
                                textFormatMode="KeyValue"
                                placeholder="Target Entity"
                                showSuggestion="true"
                                showValueHelp="true"
                                valueHelpRequest=".onValueHelpRequest1"
                                suggestionItems="{mainModel>/Entity}"
                                suggestionItemSelected=".onSuggestionItemSelected1"
                            >
                                <m:suggestionItems>
                                    <core:ListItem text="{mainModel>name}" />
                                </m:suggestionItems>
                            </m:Input>
                            <m:HBox>
                                <m:ComboBox
                                    placeholder="Type"
                                    class="sapUiSmallMarginBottom"
                                    id="associationtype"
                                    items="{ path: 'AssociationType>/Type' }"
                                >
                                    <core:ListItem
                                        key="{AssociationType>type}"
                                        text="{AssociationType>type}"
                                    />
                                </m:ComboBox>
                                <m:Button
                                    id="association"
                                    text="{i18n>button1}"
                                    press="onCreate"
                                    type="Emphasized"
                                    icon="sap-icon://collaborate"
                                />
                            </m:HBox>
                      

                            <m:Table
                                id="associationsTable"
                                mode="SingleSelectLeft"
                                items="{associationsModel>/value}"
                                visible="true"
                            >
                                <m:headerToolbar>
                                    <m:OverflowToolbar>
                                        <m:content>
                                            <m:Title
                                                text="Associations"
                                                level="H2"
                                            />
                                            <m:ToolbarSpacer />
                                           <!--  <m:Button
                                                icon="sap-icon://edit"
                                                tooltip="Edit"
                                                visible="true"
                                                press="onOpenAddDialog2"
                                            >
                                                <m:layoutData>
                                                    <m:OverflowToolbarLayoutData
                                                        priority="NeverOverflow"
                                                    />
                                                </m:layoutData>
                                </m:Button> -->
                                 <m:Button
                                    icon="sap-icon://delete"
                                    tooltip="Delete"
                                    visible="true"
                                    press=".deleteAssociation"
                                >
                                    <m:layoutData>
                                        <m:OverflowToolbarLayoutData
                                            priority="NeverOverflow"
                                        />
                                    </m:layoutData>
                                </m:Button> 
                                
                                        </m:content>
                                        <m:dependents>
                                           
                                        </m:dependents>
                                    </m:OverflowToolbar>
                                </m:headerToolbar>
                                <m:columns>
                                
                                    <m:Column>
                                        <m:Label text="Source Entity" />
                                    </m:Column>
                                    <m:Column>
                                        <m:Label text="Target Entity" />
                                    </m:Column>
                                    <m:Column>
                                        <m:Label text="Type" />
                                    </m:Column>
                                </m:columns>
                                <m:items>
                                    <m:ColumnListItem>
                                        <m:cells>
                    
                                            <m:ObjectIdentifier
                                                title="{associationsModel>entitySource1}"
                                            />
                                            <m:ObjectIdentifier
                                                title="{associationsModel>entityTarget1}"
                                            />
                                            <m:Text
                                                text="{associationsModel>type}"
                                            />
                                        </m:cells>
                                    </m:ColumnListItem>
                                </m:items>
                            </m:Table>
                            <m:Button
                                id="cds"
                                text="GenerateCds"
                                press=".OnCdsgen"
                                width="100%"
                                type="Transparent"
                                icon="sap-icon://clinical-order"
                            />
                               <m:Button
                                id="srv"
                                text="{i18n>navigateService}"
                                press="onSupplierPress"
                                width="100%"
                                type="Transparent"
                                icon="sap-icon://clinical-order"
                            />
                        </m:VBox>
                     
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>
        </sections>

    </ObjectPageLayout>
</mvc:View>
